@model IEnumerable<Feedback>
@{
    ViewData["Title"] = "Home Page";
    Layout = "_UserLayout";
}

<!-- end header section -->
<!-- start banner -->
<section class="bg-img banner-section cover-background full-screen" data-overlay-dark="5">

    <!-- Video Background -->
    <video autoplay muted loop playsinline id="bg-video" class="video-background">
        <source src="~/user/img/logos/bg_video.mp4" type="video/mp4">
        Your browser does not support the video tag.
    </video>

    <!-- Dark overlay -->
    <div class="overlay"></div>

    <div class="container h-100 z-index-9">
        <div class="display-table h-100 w-100">
            <div class="display-table-cell vertical-align-middle">
                <div class="row">
                    <div class="col-lg-7 sm-margin-40px-bottom">
                        <h1 class="font-size50 md-font-size42 sm-font-size38 xs-font-size30 font-weight-800 text-white">
                            Your Health is our Priority
                        </h1>
                        <p class="width-95 xs-width-100 font-size16 xs-display-none line-height-30 xs-font-size14 xs-line-height-26 font-size20 text-white">
                            The Preferred <br> Ambulance Company.
                        </p>
                        <div class="margin-30px-top sm-margin-25px-top">
                            <a href="/EmergencyRequests" class="butn theme white-hover">
                                <span>Get Started</span>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="story-video display-table sm-display-inherit height-100 center-col">
                            <div class="display-table-cell vertical-align-middle">
                                <div class="display-inline-block about-video vertical-align-middle">
                                    <a class="video_btn video_btn-style1 small"
                                       asp-action="Index" asp-controller="EmergencyRequests">
                                        <i class="fas fa-ambulance"></i>
                                    </a>
                                </div>
                                <div class="display-inline-block vertical-align-middle font-weight-700 font-size16 xs-font-size14 margin-25px-left text-white">
                                    Request Ambulance
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Ambulance animation starts here -->
                <!-- Ambulance animation ends here -->
            </div>
        </div>
    </div>
</section>
<!-- end banner -->
<!-- end banner -->
<!-- start about section -->
<section>
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 about-us sm-margin-40px-bottom xs-margin-30px-bottom">
                <div class="padding-90px-right md-padding-60px-right sm-no-padding-right">
                    <div class="inner-column text-center">
                        <div class="image">
                            <img src="~/user/img/content/about-1.jpg" alt="" />
                        </div>

                    </div>
                </div>
            </div>
            <div class="col-lg-6 about-us">
                <h3 class="font-size15 md-font-size14 sm-font-size13 text-uppercase margin-15px-bottom md-margin-10px-bottom font-weight-400 letter-spacing-3 word-spacing-3 text-theme-color">
                    About Us
                </h3>

                <h4 class="font-size40 md-font-size38 sm-font-size34 xs-font-size28 font-weight-500 margin-30px-bottom md-margin-25px-bottom sm-margin-20px-bottom xs-margin-15px-bottom">
                    Introducing Fully Functional
                    Ambulance Service
                </h4>
                <p class="margin-30px-bottom md-margin-25px-bottom sm-margin-20px-bottom xs-margin-15px-bottom">
                    Introducing a fully functional ambulance service designed to revolutionize emergency medical response. Our system seamlessly connects patients in need with trained EMTs, ensuring rapid and efficient transportation to medical facilities. With real-time tracking, timely notifications, and a user-friendly interface, we prioritize patient safety and comfort while enhancing overall care. Experience peace of mind knowing help is just a tap away.
                </p>

                <ul class="list-style12 margin-30px-bottom md-margin-25px-bottom sm-margin-20px-bottom xs-margin-15px-bottom">
                    <li>Car Ambulance.</li>
                    <li>Hiace Ambulance.</li>
                </ul>
                @* <h6 class="no-margin-bottom font-size18 font-weight-500 font-italic">- Umer</h6> *@
            </div>
        </div>
    </div>
</section>
